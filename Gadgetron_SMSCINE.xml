            <value>30</value>
        </property>


        <property>
            <name>mode</name>
            <value>1</value>
        </property>

        <property>
            <name>first_beat_on_trigger</name>
            <value>true</value>
        </property>


        <property>
            <name>interp_method</name>
            <value>BSpline</value>
        </property>
    </gadget>-->

    <!-- after recon processing -->
	<gadget>
        <name>ComplexToFloatAttrib</name>
        <dll>gadgetron_mricore</dll>
        <classname>ComplexToFloatGadget</classname>
    </gadget>

	<!-- 9 for mag and phase -->
